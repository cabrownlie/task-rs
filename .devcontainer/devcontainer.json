{
	"name": "Rust",
	"image": "mcr.microsoft.com/devcontainers/rust:1-1-bullseye",
	"customizations": {
		"vscode": {
			"extensions": [
				"Swellaby.rust-pack",
				"Swellaby.vscode-rust-test-adapter",
				"PolyMeilex.rust-targets"
			]
		}
	},
	"mounts": [
		{
			"source": "devcontainer-cargo-cache-${devcontainerId}",
			"target": "/usr/local/cargo",
			"type": "volume"
		}
	]
}